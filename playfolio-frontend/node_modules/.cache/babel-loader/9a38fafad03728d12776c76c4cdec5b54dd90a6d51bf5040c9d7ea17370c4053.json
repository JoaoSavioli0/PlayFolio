ponse = await axios.post(\"/v4/genres\", body, {\r\n                    headers: {\r\n                        'Accept': 'application/json',\r\n                        'Client-ID': \"i79ndcjylui2396ezi2v752sc9dze0\",\r\n                        'Authorization': `Bearer h6v8ywcqhwyyhj140u70v95rss6sga`,\r\n                        'Content-Type': 'text/plain'\r\n                    }\r\n                })\r\n\r\n                for (let data of response.data) {\r\n                    this.genresNomes[data.id] = data.name\r\n                }\r\n\r\n            } catch (error) {\r\n                console.error(\"Erro carregando generos: \" + error)\r\n            }\r\n            console.log(\"genreNomes: \" + this.genresNomes)\r\n        },\r\n    }\r\n}\r\n</script>"],"mappings":";;EACSA,KAAK,EAAC;AAAO;;EACTA,KAAK,EAAC;AAAM;;EACRA,KAAK,EAAC;AAAa;;EACfA,KAAK,EAAC;AAAuB;;EAEzBA,KAAK,EAAC;AAAiD;;EACnDA,KAAK,EAAC;AAAkB;mBAPrD;;uBACIC,mBAAA,CAgBM,OAhBNC,UAgBM,GAfFC,mBAAA,CAcM,OAdNC,UAcM,GAbFD,mBAAA,CAWM,OAXNE,UAWM,GAVFF,mBAAA,CASM,OATNG,UASM,GARFC,mBAAA,UAAa,G,kBACbN,mBAAA,CAMMO,SAAA,QAZ1BC,WAAA,CAMgGC,KAAA,CAAAC,KAAK,EAAbC,IAAI;yBAAxEX,mBAAA,CAMM,OANNY,UAMM,GALFV,mBAAA,CAGM,OAHNW,UAGM,G,0BAFFX,mBAAA,CAAuE;MAAlEH,KAAK,EAAC;IAAqD,6BAChEG,mBAAA,CAA+E;MAAzEY,GAAG,EAAEL,KAAA,CAAAM,UAAU,CAACJ,IAAI,CAACK,EAAE;MAAGjB,KAAK,EAAC;4BATlEkB,UAAA,E,6BAWwBf,mBAAA,CAAwC;MAAnCH,KAAK,EAAC;IAAsB,4B;kEAI7CG,mBAAA,CAA+B;IAA1BH,KAAK,EAAC;EAAa,4B","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}